    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="NamaLengkap, NIM" type="String" array="False" size=""/>
            <declare name="HargaBeras, Diskon, PersenDiskon, HargaSetelahDiskon, HargaBerasMawar, HargaBerasSania, HargaBerasMaknyus" type="Real" array="False" size=""/>
            <input variable="NamaLengkap"/>
            <input variable="NIM"/>
            <input variable="HargaBeras"/>
            <assign variable="HargaBerasMawar" expression="HargaBeras-(HargaBeras*(11/100))"/>
            <assign variable="HargaBerasSania" expression="HargaBeras-(HargaBeras*(14/100))"/>
            <assign variable="HargaBerasMaknyus" expression="HargaBeras-(HargaBeras*(17/100))"/>
            <output expression="NamaLengkap &amp; &quot; dengan NIM &quot; &amp; NIM &amp; &quot; ingin membeli beras seharga Rp.&quot; &amp; HargaBeras" newline="True"/>
            <output expression="&quot;Jika dia membeli beras Mawar, ia harus membayar Rp.&quot; &amp; HargaBerasMawar &amp; &quot; Setelah mendapat diskon 11%.&quot;" newline="True"/>
            <output expression="&quot;Jika dia membeli beras Sania, ia harus membayar Rp.&quot; &amp; HargaBerasSania &amp; &quot; Setelah mendapat diskon 14%.&quot;" newline="True"/>
            <output expression="&quot;Jika dia membeli beras Maknyus, ia harus membayar Rp.&quot; &amp; HargaBerasMaknyus &amp; &quot; Setelah mendapat diskon 17%.&quot;" newline="True"/>
        </body>
    </function>
